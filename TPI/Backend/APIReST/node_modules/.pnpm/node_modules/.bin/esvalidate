#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/dd486114-1144-4672-9857-38a68ba33eaa/Documentos/Facultad/UTN/Desarrollo_de_software/Repositirios/Trabajo_practico/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/mnt/dd486114-1144-4672-9857-38a68ba33eaa/Documentos/Facultad/UTN/Desarrollo_de_software/Repositirios/Trabajo_practico/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/mnt/dd486114-1144-4672-9857-38a68ba33eaa/Documentos/Facultad/UTN/Desarrollo_de_software/Repositirios/Trabajo_practico/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/esprima@4.0.1/node_modules:/mnt/dd486114-1144-4672-9857-38a68ba33eaa/Documentos/Facultad/UTN/Desarrollo_de_software/Repositirios/Trabajo_practico/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/dd486114-1144-4672-9857-38a68ba33eaa/Documentos/Facultad/UTN/Desarrollo_de_software/Repositirios/Trabajo_practico/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/mnt/dd486114-1144-4672-9857-38a68ba33eaa/Documentos/Facultad/UTN/Desarrollo_de_software/Repositirios/Trabajo_practico/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/mnt/dd486114-1144-4672-9857-38a68ba33eaa/Documentos/Facultad/UTN/Desarrollo_de_software/Repositirios/Trabajo_practico/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/esprima@4.0.1/node_modules:/mnt/dd486114-1144-4672-9857-38a68ba33eaa/Documentos/Facultad/UTN/Desarrollo_de_software/Repositirios/Trabajo_practico/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../esprima/bin/esvalidate.js" "$@"
fi
