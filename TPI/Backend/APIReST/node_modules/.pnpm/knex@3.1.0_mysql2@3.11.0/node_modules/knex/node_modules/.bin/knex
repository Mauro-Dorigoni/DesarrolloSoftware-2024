#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/mauro/UTN 2024/Desarrollo 2024/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/knex@3.1.0_mysql2@3.11.0/node_modules/knex/bin/node_modules:/mnt/c/Users/mauro/UTN 2024/Desarrollo 2024/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/knex@3.1.0_mysql2@3.11.0/node_modules/knex/node_modules:/mnt/c/Users/mauro/UTN 2024/Desarrollo 2024/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/knex@3.1.0_mysql2@3.11.0/node_modules:/mnt/c/Users/mauro/UTN 2024/Desarrollo 2024/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/mauro/UTN 2024/Desarrollo 2024/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/knex@3.1.0_mysql2@3.11.0/node_modules/knex/bin/node_modules:/mnt/c/Users/mauro/UTN 2024/Desarrollo 2024/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/knex@3.1.0_mysql2@3.11.0/node_modules/knex/node_modules:/mnt/c/Users/mauro/UTN 2024/Desarrollo 2024/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/knex@3.1.0_mysql2@3.11.0/node_modules:/mnt/c/Users/mauro/UTN 2024/Desarrollo 2024/DesarrolloSoftware-2024/TPI/Backend/APIReST/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
